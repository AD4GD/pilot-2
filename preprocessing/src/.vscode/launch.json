{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Module - protected_areas",
            "type": "python",
            "request": "launch",
            "module": "protected_areas.${fileBasenameNoExtension}",
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Python: Module - osm",
            "type": "python",
            "request": "launch",
            "module": "osm.${fileBasenameNoExtension}",
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Python: Module - enrichment",
            "type": "python",
            "request": "launch",
            "module": "enrichment.${fileBasenameNoExtension}",
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Python: Module - impedance",
            "type": "python",
            "request": "launch",
            "module": "impedance.${fileBasenameNoExtension}",
            "cwd": "${workspaceFolder}",
        }
    ]
}